<script type="text/javascript">


    var sdcaction = {
        'Account': 'AccountPay',
        'Bank': 'BankPay',
        'ConfirmPay': 'CheckInfo_ConfirmPay',
        'BankPay': 'PayInfo_BankPay',
        'AccountPay': 'PayInfo_AccountPay'
    };


    $(function () {
        $('.tabsChange li').click(function () {
            var $this = $(this);
            var clickIndex = $('.tabsChange li').index($this);
            $this.addClass('current').siblings('li').removeClass('current');
            $('.tabsItem').hide().eq(clickIndex).show();
        });

        if ($('.bankGroup li').length > +2) {
            $('.morePay').show();
        }

        $('.moreBank').bind('click', function () {
            var his = $(document.body).height();
            $(".more_box").css("height", his);
            $('.more_box').show();
            if (window._tag) {
                _tag.dcsMultiTrack('WT.event', 'MorePay', 'dcsuri', '/nopv.gif');
            }
        });
        $('.morePaytype').bind('click', function () {
            $(this).closest('li').hide();
            $(this).closest('li').prev().show();
        });
        $('.s_gb').click(function () {
            $('.more_box').hide();
        });

        $('.popClose, .ywczf, .cxxzyh').click(function () {
            $('#popPay').hide();
            $('#mask').hide();
        });

        $('.bankList').on('click', 'li img', function () {
            $('input[type="radio"]', this.parentNode).click();
        });
        $('.creditcard_list input[name=bankAbbr]:disabled').closest('li').hover(function () {
            var $t = $(this);
            var position = $t.offset(),
                top = position.top,
                left = position.left;
            $t.closest('ul').prev().html('<img src="/paygwsharding/resources/images/icon_banktips.jpg" align="absmiddle">&nbsp;&nbsp;' + $t.find('.tips_text').text()).css({
                'top': (top + 40),
                'left': (left + 28)
            }).show();
        }, function () {
            var $t = $(this);
            $t.closest('ul').prev().html('').css({'top': '0', 'left': '0'}).hide();
        });
        $('.creditcard_list').on('click', '.show_desc', function () {
            //http://pay.shop.staff.139.com/paygw/jsp/queryRates.jsp?bankAbbr=

            //$('.creditcard_list').children('li').bind('click','.show_desc',function(){
            var url = $(this).attr('url');

            $.post(url, function (resq) {
                $('.creditcard .description_box').html(resq);
                //   		var r = {code: resq.substr(0,1),msg: resq.substr(2)};
                //   		if(r.code){alert(r.msg)}else{
                //   			$('.creditcard .description_box').html(r.msg);
                //   		}
            });
        });

    });

    function Regular(btn) {
        var $v = $(btn.form).find('[name="bankAbbr"]:checked');
        var tag = false;
        var bank = null;

        if ($v.length > 0) {
            tag = true;
            bank = $v.eq(0).val();
        }

        if (!tag) {
            alert("请选择支付方式");
            return false;
        }
        $('#popPay').show();
        $('.more_box').hide();
        $('#mask').height($(document).height() > $(window).height() ? $(document).height() : $(window).height()).show();

        if (typeof (_tag) != "undefined") {
            _tag.dcsMultiTrack('WT.category', 415984075962525703 + "_" + 11, 'WT.event', 'CheckInfo_ConfirmPay', 'DCS.dcsuri', '/nopv.gif');
        }
        if ('WXPAY' == bank) {
            showWxQrcode(btn);
            return false;
        } else {
            return tag;
        }
    }

    function flshpage() {
        window.location.reload();
    }

    //储蓄卡+信用卡
    var ajaxstatus;

    function bankPay() {
        if (window._tag) {
            _tag.dcsMultiTrack('WT.event', sdcaction.Bank, 'dcsuri', '/nopv.gif');
        }
        if (typeof (_tag) != "undefined") {
            _tag.dcsMultiTrack('WT.category', "415984075962525703_11", 'WT.event', sdcaction.BankPay, 'DCS.dcsuri', '/nopv.gif');
            var userId = '1255491269' + "";
            var phoneNum = '13504410153' + "";
            if ($('#districtbank').find('li').length == 1 && ($('#morebank').find('li').length == 0)) {
                $('.load_jz').show();
                if (ajaxstatus != null && ajaxstatus != "") {
                    if (ajaxstatus.state() === 'pending') {
                        return false;
                    }
                } else {
                    ajaxstatus = $.ajax({
                        type: "POST",
                        url: "/paygwsharding/getRecommendPayBankAjax",
                        dataType: "JSON",
                        data: {
                            "userId": userId,
                            "phoneNum": phoneNum,
                            "timestamp": new Date().getTime()
                        },
                        success: function (data) {
                            $('.load_jz').hide();
                            if (data == null) {
                                alert("查询失败，请重试");
                            } else {
                                var historyli = "";
                                for (var key in data.historybank) {
                                    historyli = historyli + "<li><label> <input type='radio' name='bankAbbr' value=" + key + " /> <img src='/paygwsharding/resources/images/bank/" + key + ".gif' /></label></li>";
                                    $('#historybank').css("display", "block");
                                }
                                if (historyli != "") {
                                    $('#historybank').append(historyli);
                                }

                                var bigli = "";
                                for (var key in data.bigbank) {
                                    bigli = bigli + "<li><label> <input type='radio' name='bankAbbr' value=" + key + " /> <img src='/paygwsharding/resources/images/bank/" + key + ".gif' /></label></li>";
                                }
                                $('#bigbank').append(bigli);

                                var districtli = "";
                                for (var key in data.districtbank) {
                                    districtli = districtli + "<li><label> <input type='radio' name='bankAbbr' value=" + key + " /> <img src='/paygwsharding/resources/images/bank/" + key + ".gif' /></label></li>";
                                }
                                $('#districtbank').prepend(districtli);

                                var moreli = "";
                                //$('#morebank li').remove();
                                for (var key in data.morebank) {
                                    moreli = moreli + "<li><label> <input type='radio' name='bankAbbr' value=" + key + " /> <img src='/paygwsharding/resources/images/bank/" + key + ".gif' /></label></li>";
                                }
                                $('#morebank').append(moreli);

                                $('.moreBank').css("display", "block");
                            }
                        },
                        error: function (data) {
                            alert('查询失败，请重试');
                        }
                    });
                }
            }
        }
    }

