    //微信支付
    function showWxQrcode(btn) {
        var $form = $(btn.form);
        $.ajax({
            url: '/paygwsharding/mobileAndBankPayAjax',
            async: false,
            data: $form.serialize(),
            success: function (r) {
                if (r.code) {
                    alert(r.msg)
                } else {
                    if (r.type == 'qrcode') {
                        $('#wxArea').show();
                        $('#wxArea dt').html('<img src="' + r.url + '" />');
                        timeJs(r.url);
                    } else if (r.type == 'redirect') {
                        location.href = r.url;
                    }
                }
            },
            dataType: 'json',
            error: function (r) {
                alert('支付失败，请重试');
                //$('#wxArea').show();
                //$('#wxArea dt').html('<img src="'+$('.orderSuccess .qrcode').attr('src')+'" />');
            }
        });
    }

