    function timeJs(imgurl) {
        setTimeout(function () {
            $('#wxArea dt').html('<img src="/paygwsharding/resources/images/qrcodeexpire.jpg" />').find('img').click(function () {
                $('#wxArea dt').html('<img src="' + imgurl + '&_=' + Math.random() + '" />');
                timeJs(imgurl);
            });
        }, 90 * 1000);
    }

    var wxTimer = setInterval(function () {
