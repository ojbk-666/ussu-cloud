        var uri = require('uri');
        var pid = new uri(location.href).getQueryParamValue("f");
        var _p = new uri(location.href).getQueryParamValue("p");
        if (pid == "rechargejf" && _p == "1" && data.userType == "1") {
            var req_data = {};
            //处理url参数,将url中的所有参数都原数传递给接口
