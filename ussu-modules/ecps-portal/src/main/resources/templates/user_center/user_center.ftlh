                        _Constant.RCURRFUNCODE = (typeof _obj != "undefined" && _obj != null) ? _obj.funcode : null;
                        //修改面包屑去菜单的名称 @2016-04-16
                        var obj = comSet.getObjectByName(item, 0);
                        if (_Constant.CURRENTMENUS != '' && typeof _Constant.CURRENTMENUS != 'undefined') {
                            for (var i = 0; i < _Constant.CURRENTMENUS.menu.length; i++) {
                                if (typeof _Constant.CURRENTMENUS.menu[i].child != 'undefined') {
                                    for (var j = 0; j < _Constant.CURRENTMENUS.menu[i].child.length; j++) {
                                        if (item == _Constant.CURRENTMENUS.menu[i].child[j].inx) {
                                            obj = _Constant.CURRENTMENUS.menu[i].child[j];
