		param.city_id = 431;
		$.getJSON('/ajax/detail/expense.json',param,function(r){
			if(r.code){alert(r.msg)}else{
				var	tmp_tabs = '',
					tmp_content = '<ul class="pagesList marginTop20 clearfix withhover withcurrent" style="display:none;">',
					tmp_content_current = '<ul class="pagesList marginTop20 clearfix withhover withcurrent">',
					i=0,
					j=0,
					$taocan_content = $('.taocan_content').html('')
				;
				$tabs.css({width:'auto'}).html('');
				$('.taocanBox').show();
				if(!r.data.goods || (r['data']['goods'].length!=undefined&&r['data']['goods'].length==0)){
					$taocan_content.html('<div style="text-align:center;font-size:16px;height:100px;line-height:100px">'+pop_tips+'</div>')
						.next().hide().next().hide();
