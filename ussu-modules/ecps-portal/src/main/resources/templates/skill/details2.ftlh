					cache[item.goods_id] = item;
					if(i==0){
						tmp_tabs+=template_tabs_current.exec(item);
					}else{
						tmp_tabs+=template_tabs.exec(item);
					}
					i++;
				});
				$tabs.html(tmp_tabs);
				//计算tabs总体宽度
				var tabsWidth = 0;
				var lengthArray = [];
				var $tabButtons = $tabs.find('li').each(function(tabs_i,item){
					tabsWidth += $(item).outerWidth(true);
					lengthArray.push($(item).outerWidth(true));
				});
				//tabs左右控制
				var $hiddenArea = $tabs.closest('.hidden');
				var tabsHiddenWidth = $hiddenArea.width();
				var tabindex = 0;
				var $cur_left_btn = null;
				var $cur_right_btn = null;
				$tabsControl.click(function(){
					var $t = $(this),
						realEnd = 0,
						range = 0,
						$tab = null,
						tabwidth = 0,
						scrollLeft = 0 ;
					if($t.is(".turn_r")){
						realEnd = $hiddenArea.scrollLeft() + tabsHiddenWidth;
						for(var n = 0;n<lengthArray.length;n++){
							range += lengthArray[n];
							if(range > realEnd){
								$tab = $tabButtons.eq(n);
								tabwidth = lengthArray[n]
								break;
							}
						}
						scrollLeft = range - tabsHiddenWidth;
					}else{
						realEnd = $hiddenArea.scrollLeft();
						for(var n = 0;n<lengthArray.length;n++){
							range += lengthArray[n];
							if( range >= realEnd){
								$tab = $tabButtons.eq(n);
								tabwidth = lengthArray[n];
								break;
							}
						}
						scrollLeft = range - tabwidth;
					}
					$hiddenArea.scrollLeft(scrollLeft);
					if($hiddenArea.scrollLeft()>0){
						$tabsControl.filter('.turn_l').show();
					}else{
						$tabsControl.filter('.turn_l').hide();
					}
					if(realEnd >= tabsWidth ||  tabsWidth <= tabsHiddenWidth ){
						$tabsControl.filter('.turn_r').hide();
					}else{
						$tabsControl.filter('.turn_r').show();
					}
				}).eq(0).click();

				$tabs.width(tabsWidth + 1); //由于浏览器计算宽度小数点的问题，需要+1像素补充，不然样式会掉下去
				$.each(r.data.goods||{},function(inx,item){
					var tmp="";
					if(j==0){
						tmp=tmp_content_current;
					}else{
						tmp=tmp_content;
					}
					$.each(item.sku||{},function(inx,jitem){
						jitem.index=jitem.model_id
						tmp+=template_content.exec(jitem);
						jitem.goods_name=item.goods_name;
						//item['sku'][jitem.model_id+1]=jitem;
						//item['sku'][jitem.model_id+1]['goods_name']=item.goods_name;
						//item.sku.splice(inx,1);
					});
					if(j==0){
						$(tmp+'</ul>').appendTo($taocan_content).find('li').eq(0).click();
					}else{
						$(tmp+'</ul>').appendTo($taocan_content);
					}
					j++;
				});
				if(ext && ext.model_id){
					if(ext.goods_id){
						$tabButtons.filter('[goods_id='+ext.goods_id+']').click();
