function query_heyue_stock(contract) {
    var cstock =  window.ENVOBJ.phone.cstock||{},  // 当前终端库存
        cskuid = window.ENVOBJ.phone.csku.info.model_id,  // 当前终端skuid
        contract_id = contract.contract_id, // 当前选择的合约id
        phone_stock = window.ENVOBJ.cur_stock||{},
        final_stock = {};

    final_stock[cskuid] = cstock;

    if(!contract) return;
    var promo_types = ',21,3,';
    if(contract.promotion) { // 如果合约参与活动，那么把活动信息存入 ENVOBJ.choice.info 中，否则删除 ENVOBJ.choice.info 中的 promotion
        ENVOBJ.choice.info.promotion = contract.promotion;
    } else if(ENVOBJ.choice.info.promotion && promo_types.indexOf(","+ENVOBJ.choice.info.promotion.sale_type+",")>-1 ) {
        //合约档限量购和合约机限时购 ，活动在合约档上，如果档位上没有活动，删除sku的promotion
        delete ENVOBJ.choice.info.promotion;
    }

    var is_shudi = (3 == contract.all_used_flag),
        is_xianliang = (contract.promotion && 21 == contract.promotion.sale_type);

    // 如果既不是属地合约，也不是合约机限量购，那么直接将商品信息存入cart
    if(!is_shudi && !is_xianliang) {
        $.addToCart({
            'goods_id': 1077764 ,
            'goods_type' : 70000,
            'stock':phone_stock
        });
        return;
    }

    /**
     * 第一种情况  属地库存 sale_type = 5   sku_id=232434
     * 第二种情况  合约档限购的库存 sale_type =6   sku_id= -1 detail_id= 32424
     * 第三种情况  合约档限购属地合约一起的 stock_type =6 sku_id=232434（属地的skuid），detail_id= 32424（合约档限购的活动team_id）
     */
    var sale_type_shudi = 5,
        sale_type_xiangliang = 6,
        sku_id = contract_id,
        sku_id_xiangliang = -1;
    var param = {
        sale_type: sale_type_shudi
    };
    var cart = {
        'goods_id': 1077764 ,
        'goods_type' : 70000
    };
    // 合约限量购
    if(is_xianliang) {
        param.sale_type = sale_type_xiangliang;
        param.sku_id = sku_id_xiangliang;
        param.detail_id = contract.promotion.team_id;
        param.promo_type = contract.promotion.sale_type;
    }
    // 属地合约
    if(is_shudi) {
        param.sku_id = sku_id;
    }
    $.getJSON('/ajax/detail/getstock.json', param, function(r){
        var stock = {}, contract_flag = false, sku_id = param.sku_id;
        if (r.code == 0){
            stock = r.data;
            var contract_stock = stock[sku_id]; // 属地合约库存/合约限量购活动库存
            var _cstock = phone_stock[cskuid]; // 终端库存
            if(_cstock && contract_stock) {
                // 终端有库存，所选合约档无库存。前端立即购买置灰。不显示到货提醒。
                // 取终端库存及合约库存的小者
                if(_cstock.stock > contract_stock.stock) { // 当终端有库存，选择合约档无库存时，存放标识
                    final_stock[cskuid] = contract_stock;
					contract_flag = true;
                }
                // 取lower_limit较大者
                final_stock[cskuid].lower_limit = Math.max(parseInt(_cstock.lower_limit), parseInt(contract_stock.lower_limit));
            }
            if(is_xianliang) {
                cart.is_xianliang_heyue = true;
            }
            cart.stock = final_stock;
            cart.contract_flag = contract_flag;
        }
        $.addToCart(cart);
    });
}
});



