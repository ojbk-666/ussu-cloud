                                $(e1).addClass('color-2');
                                if (!$(e1).parent().parent().parent().children().eq(0).hasClass('li-active')) {
                                    $(e1).parent().parent().parent().children().eq(0).trigger('click');
                                }

                            }
                        });
                        var _obj = comSet.getObjectByName(item, 0);
                        _Constant.CURRFUNC = item;
                        _Constant.RCURRFUNCODE = (typeof _obj != "undefined" && _obj != null) ? _obj.funcode : null;
                        //修改面包屑去菜单的名称 @2016-04-16
                        var obj = comSet.getObjectByName(item, 0);
                        if (_Constant.CURRENTMENUS != '' && typeof _Constant.CURRENTMENUS != 'undefined') {
                            for (var i = 0; i < _Constant.CURRENTMENUS.menu.length; i++) {
                                if (typeof _Constant.CURRENTMENUS.menu[i].child != 'undefined') {
                                    for (var j = 0; j < _Constant.CURRENTMENUS.menu[i].child.length; j++) {
                                        if (item == _Constant.CURRENTMENUS.menu[i].child[j].inx) {
                                            obj = _Constant.CURRENTMENUS.menu[i].child[j];
                                            break;
                                        }
                                    }
                                }
                            }
                        }
                        _Constant.CURRFUNCODE = obj.funcode;
                        sspart.getCrumbPart(item, obj);
                        if (obj.funcode != '042000') {//当宽带充值时不调用公共的温馨提示
                            sspart.getWarmTipPart(app.data[0].mobile, '', obj.funcode);
                        }
                        //运营位设置   @lujx 2017-09-04
                        var paras = {
                            'areaID': obj.funcode,
                            'provCode': '100',
                            'channel': '00',
                            mobile: app.data[0].mobile
                        };
                        sspart.getOperationInfo(paras, obj);
                    }
                });
            });
        });
        //modified by zhouhong 20161128 判断如果是积分支付结果页则切换帐号到充值的手机号 start
        //app.initpage('stcMenu',{mobile:data.loginValue,data:data});   //引入左侧菜单区域，增加菜单选中的效果
        var uri = require('uri');
        var pid = new uri(location.href).getQueryParamValue("f");
        var _p = new uri(location.href).getQueryParamValue("p");
        if (pid == "rechargejf" && _p == "1" && data.userType == "1") {
            var req_data = {};
            //处理url参数,将url中的所有参数都原数传递给接口
            if (__params.length > 0) {//__params在constant.js中
                for (var k in __params) {
                    req_data[__params[k][0]] = __params[k][1];
                }
            }
            require(["/i/service/model/recharge_4d3f3eb.js"], function (recharge) {
                recharge.getPayCallBack(req_data, function (pay_data) {
                    app.initpage('stcMenu', {mobile: pay_data.mobile, data: data});   //引入左侧菜单区域，增加菜单选中的效果
                }, function (code, msg) {
                    //own.showSelectMobile(userInfo);
                    app.initpage('stcMenu', {mobile: data.loginValue, data: data});
                }, function (status, msg) {
                    //own.showSelectMobile(userInfo);
                    app.initpage('stcMenu', {mobile: data.loginValue, data: data});
                });
            });
        } else {
            app.initpage('stcMenu', {mobile: data.loginValue, data: data});   //引入左侧菜单区域，增加菜单选中的效果
        }
        //modified by zhouhong 20161128 如果是积分支付结果页则切换帐号到充值的手机号 end
        if (typeof pid == 'undefined' || pid == '' || pid == 'home') {
            pid = 'home';
            $('#stc_first_loc').hide();
            $('#stc_first_arrow').hide();
            $('#stc_second_loc').html('个人中心');
            app.initpage(pid);
        }
        //插码调用
        stcEcode.startGlobalTrack(data);//插码,基本码
        require(["/i/apps/pageapps/pheader/js/head_456f66f.js", "/i/apps/pageapps/pheader/js/tail_65e0761.js", "/i/apps/pageapps/pheader/tpl/easyentry_b448db8.js"], function (head, tail, easy) {
