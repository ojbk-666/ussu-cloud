                        data.provice = '999'; //未登录的省份编码为999
                    }
                    var encryptToken = 'yxfmxifprj';
                    if (!$.isEmptyObject(data.encryptToken)) {
                        encryptToken = data.encryptToken;
                    }
                    sessionStorage.setItem('encryptToken', encryptToken);
                    getInfoSucc(data, app, stcDic);
