            jqaop.aop.after({target: app, method: 'initpage'}, function (invocation) {
                var arg = this.initpage.arguments[0];
                //by zhongbin 2017/9/25 设置f码
                $('#mainContain').attr('data-f', arg);
                require(["/i/apps/pageapps/pwarm/stcSpecialPart_ba7ece1.js", "/i/service/model/componet_set_42306f9.js"], function (sspart, comSet) {
                    var item = arg;
                    //首次登录生活交费界面重置标识20170808 start
                    if (item == 'rechargelife') {
                        _Constant.FIRSTLOGINLIFEPAGE = true;
                    }
                    //end
                    if (comSet.isMenuItem(item)) {//判断是否属于菜单里边的内容
                        //修改点击后一级菜单不展开，同时二级菜单无选中颜色的问题 @2016-06-21
