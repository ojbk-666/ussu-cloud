                            mobile: stcDic.getDefaultUserInfo().loginValue,
                            data: stcDic.getDefaultUserInfo()
                        });
                        app.initpage("stcError");
                    }
                }, function () {
                    sessionStorage.setItem('encryptToken', 'yxfmxifprj');
                    var posi = new uri(location.href).getQueryParamValue("f");
                    var item = {"funCode": "999001", "operDesc": posi};//代表走进error的打点 @2016-09-22
                    var stcLog = require('stclog');
                    stcLog.info(null, item);
