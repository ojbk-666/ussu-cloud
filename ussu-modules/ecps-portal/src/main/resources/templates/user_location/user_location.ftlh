    //将获取用户信息的替换掉。 @2016-06-26
    var getInfoSucc = function (data, app, stcDic) {
        $('#stc_shop_link').attr('href', stcDic.getShopLink());
        $('#stc_first_loc').attr('href', stcDic.getLoginaddr());
        //统一处理温馨提示及面包屑的问题，通过aop切面的形式
