                } else {
                    req.userId = app.data[0].data.loginValue;
                    req.userType = app.data[0].data.userType;
                    req.tempKey = $.cookie('cart_code_key');
                    shoppingcar.mergeShoppingCarGoods(req);//已经登录,那么先合并购物车,再去调用购物车个数接口
                }
                shoppingcar.init();//购物车初始函数,主要是为dom元素绑定事件
            });
//			---------------end
        });
    };

    $(function () {
